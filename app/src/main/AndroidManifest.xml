<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.tibet.cares.tibetairapp"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".common.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.InitActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity"/>
        <activity android:name=".activity.FilterFlightNumActivity"/>
        <activity android:name=".activity.TicketSearchFlightActivity">
        </activity>
        <activity android:name=".activity.TicketSearchFlightDetailActivity">
        </activity>
        <activity android:name=".activity.TicketDetailActivity">
        </activity>
    </application>

</manifest>
